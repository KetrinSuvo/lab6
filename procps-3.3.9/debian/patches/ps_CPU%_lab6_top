-{"pcpu",      "%CPU",    pr_pcpu,     sr_pcpu,    4,   0,    U98, ET|RIGHT}, /*%cpu*/
+//{"pcpu",      "%CPU",    pr_pcpu,     sr_pcpu,    4,   0,    U98, ET|RIGHT}, /*%cpu*/
+{"pcpu",      "%CPU",    mypr_pcpu,     sr_pcpu,    4,   0,    U98, ET|RIGHT}, /*my cpu%*/
