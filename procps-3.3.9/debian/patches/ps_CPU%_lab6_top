+S["NCURSESW_LIBS"]=""
+S["NCURSESW_CFLAGS"]=""
+S["NCURSES_LIBS"]="-lncurses  "
+S["NCURSES_CFLAGS"]="-I/usr/include/ncursesw  "
+S["WITH_NCURSES_FALSE"]="#"
+S["WITH_NCURSES_TRUE"]=""
+S["WITH_WATCH8BIT"]=""
+S["POSUB"]="po"
+S["LTLIBINTL"]=""
+S["LIBINTL"]=""
