+include ./$(DEPDIR)/pidof.Po
+include ./$(DEPDIR)/pmap.Po
+include ./$(DEPDIR)/pwdx.Po
+include ./$(DEPDIR)/skill.Po
+include ./$(DEPDIR)/slabtop.Po
+include ./$(DEPDIR)/strutils.Po
+include ./$(DEPDIR)/sysctl.Po
+include ./$(DEPDIR)/tload.Po
