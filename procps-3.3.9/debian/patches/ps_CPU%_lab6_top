+abs_srcdir = /home/ketrin/lab6/procps-3.3.9
+abs_top_builddir = /home/ketrin/lab6/procps-3.3.9
+abs_top_srcdir = /home/ketrin/lab6/procps-3.3.9
+ac_ct_AR = ar
+ac_ct_CC = gcc
+ac_ct_CXX = g++
+ac_ct_DUMPBIN = 
+am__include = include
+am__leading_dot = .
+am__quote = 
