+S["LTLIBICONV"]="-liconv"
+S["LIBICONV"]="-liconv"
+S["MSGMERGE"]="/usr/bin/msgmerge"
+S["XGETTEXT"]="/usr/bin/xgettext"
+S["GMSGFMT"]="/usr/bin/msgfmt"
+S["MSGFMT"]="/usr/bin/msgfmt"
+S["USE_NLS"]="yes"
+S["MKINSTALLDIRS"]="$(top_builddir)/./mkinstalldirs"
+S["usrbin_execdir"]="${exec_prefix}/usr/bin"
+S["POW_LIB"]=""
+S["LIBOBJS"]=""
+S["CXXCPP"]="g++ -E"
+S["OTOOL64"]=""
+S["OTOOL"]=""
+S["LIPO"]=""
+S["NMEDIT"]=""
