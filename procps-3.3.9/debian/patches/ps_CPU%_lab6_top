+#	$(AM_V_CC)source='$(top_srcdir)/lib/fileutils.c' object='fileutils.obj' libtool=no \
+#	DEPDIR=$(DEPDIR) $(CCDEPMODE) $(depcomp) \
+#	$(AM_V_CC_no)$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(AM_CPPFLAGS) $(CPPFLAGS) $(AM_CFLAGS) $(CFLAGS) -c -o fileutils.obj `if test -f '$(top_srcdir)/lib/fileutils.c'; then $(CYGPATH_W) '$(top_srcdir)/lib/fileutils.c'; else $(CYGPATH_W) '$(srcdir)/$(top_srcdir)/lib/fileutils.c'; fi`
+
+nsutils.o: $(top_srcdir)/lib/nsutils.c
+	$(AM_V_CC)$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(AM_CPPFLAGS) $(CPPFLAGS) $(AM_CFLAGS) $(CFLAGS) -MT nsutils.o -MD -MP -MF $(DEPDIR)/nsutils.Tpo -c -o nsutils.o `test -f '$(top_srcdir)/lib/nsutils.c' || echo '$(srcdir)/'`$(top_srcdir)/lib/nsutils.c
+	$(AM_V_at)$(am__mv) $(DEPDIR)/nsutils.Tpo $(DEPDIR)/nsutils.Po
